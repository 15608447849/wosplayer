<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wosplayer"
    android:versionCode="9"
    android:versionName="9.0"
    android:screenOrientation="portrait">

    <application
        android:name=".app.WosApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:sharedUserId="android.uid.system"
        android:hardwareAccelerated="true">

        <!-- 启动activity  <intent-filter>
                <action android:name="com.wos.launcher.show" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>-->
        <activity android:name=".activity.DisplayActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <!-- 通讯服务 android:process=":remote_communication" -->
        <service android:name=".service.CommunicationService" android:process=":remote_communication"/>
        <!-- 接受通讯服务的广播 -> 派发任务-->
        <receiver android:name=".cmdBroadcast.CmdPostTaskCenter">
            <intent-filter>
                <action android:name="com.post.cmd.broad" />
            </intent-filter>
        </receiver>

        <!-- 接受下载 队列 android:process=":remote_loader" -->
        <service android:name=".loadArea.kernal.loaderManager" android:process=":remote_loader"/>

        <!-- 下载完成 广播-->
        <receiver android:name=".loadArea.kernal.completeTaskListBroadcast" >
            <intent-filter>
                <action android:name="com.complete.taskList"/>
            </intent-filter>
        </receiver>
        <!-- 通知ui执行者 广播接受-->
        <receiver android:name=".Ui.uiBroadcast.UibrocdCastReceive" >
            <intent-filter>
                <action android:name="com.wos.ui.refresh" />
            </intent-filter>
        </receiver>



        <!--监听服务  android:enabled="true"  android:process=".MonitorService"-->
        <service android:name="com.wosplayer.service.MonitorService" android:process=":remote_monitorService"/>
        <receiver android:name=".service.RestartApplicationBroad" >
            <intent-filter>
                <action android:name="com.wos.restartbroad" />
            </intent-filter>
        </receiver>

        <!-- log-->
        <!-- 记录日志sever  android:process=":remote_log_upload"-->
        <service
            android:name=".service.serviceLog"  android:process=":remote_log_upload"/>
        <!-- 日志上传  android:process=":remote_log_upload"-->
        <service
            android:name=".service.logUploadService"  android:process=":remote_log_upload"/>

        <!-- 开机自启动广播-->
        <receiver android:name="com.wosplayer.service.StartSelfBroad" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

        <!-- 拨叉sdcard 广播 -->
        <receiver android:name="com.wos.SdcardBroad">
            <intent-filter android:priority="1000">
                <action android:name="android.intent.action.MEDIA_MOUNTED"/>
                <action android:name="android.intent.action.MEDIA_EJECT" />
                <data android:scheme="file"/>
            </intent-filter>
        </receiver>

        <!-- 安装卸载等广播-->
        <receiver android:name="com.wosplayer.service.installBroad">
            <intent-filter>
                <data android:scheme="package" />
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_CHANGED" />
                <action android:name="android.intent.action.PACKAGE_INSTALL" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />
                <action android:name="android.intent.action.PACKAGE_DATA_CLEARED" />
                <action android:name="android.intent.action.PACKAGE_FIRST_LAUNCH" />
                <action android:name="android.intent.action.PACKAGE_RESTARTED" />
                <action android:name="android.intent.action.PACKAGE_VERIFIED" />
                <!--<action android:name="android.intent.action.BOOT_COMPLETED"/>-->
                <!--<category android:name="android.intent.category.HOME" />-->
            </intent-filter>
        </receiver>
    </application>


    <!-- 权限 -->
    <uses-permission android:name="android.permission.INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" /> <!-- 开机启动权限-->
    <uses-permission android:name="android.permission.WRITE_APN_SETTINGS"/>
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.hardware.usb.host" />
    <uses-permission android:name="android.permission.REBOOT" />
    <!-- 允许设置时区-->
    <uses-permission android:name="android.permission.SET_TIME_ZONE" />
    <uses-permission android:name="android.permission.SET_TIME"/>
    <uses-permission android:name="android.permission.INTERNET" />
