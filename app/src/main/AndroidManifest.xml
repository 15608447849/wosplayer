<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wosplayer"
    android:versionCode="9"
    android:versionName="9"
    android:screenOrientation="portrait">

    <application
        android:name=".app.wosPlayerApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:sharedUserId="android.uid.system"
        android:hardwareAccelerated="true">

        <!-- 启动activity -->
        <activity android:name=".activity.DisplayActivity"
            android:launchMode="singleTask">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.wos.launcher.show" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!-- 申请终端Activity-->
        <activity android:name="wosTools.ToolsActivity" android:label="MBS_TOOLS" />



        <!-- 通讯服务 -->
        <service android:name=".service.CommunicationService" />
        <!-- 通讯服务 重新链接 广播-->
        <receiver android:name=".service.CommunicationReconnectBroad">
            <intent-filter>
                <action android:name="com.communication.reconncetion.broad"></action>
            </intent-filter>
        </receiver>

        <!-- 接受通讯服务的广播 -> 派发任务-->
        <receiver android:name=".cmdBroadcast.CmdPostTaskCenter">
            <intent-filter>
                <action android:name="com.post.cmd.broad"></action>
            </intent-filter>
        </receiver>


        <!-- 接受下载 队列 -->
        <service android:name=".loadArea.loaderManager" />


        <!-- 下载完成 广播-->
        <receiver android:name=".loadArea.completeTaskListBroadcast">
            <intent-filter>
                <action android:name="com.complete.taskList"/>
            </intent-filter>
        </receiver>
        <!-- 通知ui执行者 广播接受-->
        <receiver android:name=".Ui.uiBroadcast.UibrocdCastReceive">
            <intent-filter>
                <action android:name="com.wos.ui.refresh" />
            </intent-filter>
        </receiver>



        <!--监听服务  android:enabled="true"  android:process=".MonitorService"-->
        <service android:name="com.wosplayer.service.MonitorService" />
        <receiver android:name=".service.RestartApplicationBroad">
            <intent-filter>
                <action android:name="com.wos.restartbroad" />
            </intent-filter>
        </receiver>

        <!-- log-->
        <!-- 记录日志sever-->
        <service
            android:name=".service.serviceLog" />
        <!-- 日志上传-->
        <service
            android:name=".service.logUploadService"/>

        <!-- 开机自启动广播-->
        <receiver android:name="com.wosplayer.service.StartSelfBroad">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>


        <!-- 安装卸载等广播-->
        <receiver android:name="com.wosplayer.service.installBroad">
            <intent-filter>
                <data android:scheme="package" />
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_CHANGED" />
                <action android:name="android.intent.action.PACKAGE_INSTALL" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />
                <action android:name="android.intent.action.PACKAGE_DATA_CLEARED" />
                <action android:name="android.intent.action.PACKAGE_FIRST_LAUNCH" />
                <action android:name="android.intent.action.PACKAGE_RESTARTED" />
                <action android:name="android.intent.action.PACKAGE_VERIFIED" />
                <!--<action android:name="android.intent.action.BOOT_COMPLETED"/>-->
                <!--<category android:name="android.intent.category.HOME" />-->
            </intent-filter>
        </receiver>
    </application>


    <!-- 权限 -->
    <uses-permission android:name="android.permission.INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" /> <!-- 开机启动权限-->
    <uses-permission android:name="android.permission.WRITE_APN_SETTINGS"/>
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.hardware.usb.host" />
    <uses-permission android:name="android.permission.REBOOT" />
    <!-- 允许设置时区-->
    <uses-permission android:name="android.permission.SET_TIME_ZONE" />
    <uses-permission android:name="android.permission.SET_TIME"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SHUTDOWN" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
